<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springreact.mapper.WarehousingRecordMapper">
	<select id="selectGetList" resultType="com.example.springreact.domain.WarehousingRecord" parameterType="string">
    <![CDATA[
		SELECT * FROM warehousing_record wr
						  JOIN item i ON wr.item_code = i.item_code
		WHERE warehousing_code LIKE '%'||#{code}||'%'
]]>
</select>
</mapper>